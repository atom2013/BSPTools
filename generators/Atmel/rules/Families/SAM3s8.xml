<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>SAM3s8</Name>
	<DeviceRegex>^SAM3S[8D].*</DeviceRegex>
	<FamilySubdirectory>SAM3s8</FamilySubdirectory>
	<PrimaryHeaderDir>$$BSPGEN:INPUT_DIR$$</PrimaryHeaderDir>
	<StartupFileDir>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\sam3s8\include\sam3s8.h</StartupFileDir>
  	<CoreFramework>
	  	<CopyJobs>
			<CopyJob>
				<SourceFolder>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\sam3s8</SourceFolder>
				<FilesToCopy>-*gcc*;-*iar*;*.h;*.c</FilesToCopy>
				<TargetFolder>Utils</TargetFolder>
				<ProjectInclusionMask>*.c</ProjectInclusionMask>
				<AutoIncludeMask>*.h</AutoIncludeMask>
				<SimpleFileConditions>
				</SimpleFileConditions>
			</CopyJob>		
		</CopyJobs>	
	</CoreFramework>
	<Subfamilies>			
	</Subfamilies>
	     <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix</Key>
          <Value>sam3s8</Value>
        </SysVarEntry>
		<SysVarEntry>
          <Key>com.sysprogs.atmel.sam32.dirutils</Key>
          <Value>sam</Value>
        </SysVarEntry>
		     <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix_sam0</Key>
          <Value>no</Value>
        </SysVarEntry>
     </AdditionalSystemVars>

</FamilyDefinition>
