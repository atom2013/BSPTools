<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>SAMV71</Name>
	<DeviceRegex>^ATSAMV71.*</DeviceRegex>
	<FamilySubdirectory>SAMV71</FamilySubdirectory>
	<PrimaryHeaderDir>$$BSPGEN:INPUT_DIR$$</PrimaryHeaderDir>
	<StartupFileDir>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\samv71\include\samv71.h</StartupFileDir>
  	<CoreFramework>
	  	<CopyJobs>
			<CopyJob>
				<SourceFolder>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\samv71</SourceFolder>
				<FilesToCopy>-*gcc*;-*iar*;*.h;*.c</FilesToCopy>
				<TargetFolder>Utils</TargetFolder>
				<ProjectInclusionMask>*.c</ProjectInclusionMask>
				<AutoIncludeMask>-*component*;-*instance*;-*pio*;*.h</AutoIncludeMask>
				<SimpleFileConditions>
				</SimpleFileConditions>
			</CopyJob>		
		</CopyJobs>	
	</CoreFramework>
	<Subfamilies>			
	</Subfamilies>
	 <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix</Key>
          <Value>samv71</Value>
        </SysVarEntry>
		
		<SysVarEntry>
          <Key>com.sysprogs.atmel.sam32.dirutils</Key>
          <Value>sam</Value>
        </SysVarEntry>
		     <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix_sam0</Key>
          <Value>no</Value>
        </SysVarEntry>
     </AdditionalSystemVars>
</FamilyDefinition>
