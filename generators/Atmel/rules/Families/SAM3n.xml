<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>SAM3n</Name>
	<DeviceRegex>^ATSAM3N.*</DeviceRegex>
	<FamilySubdirectory>SAM3n</FamilySubdirectory>
	<PrimaryHeaderDir>$$BSPGEN:INPUT_DIR$$</PrimaryHeaderDir>
	<StartupFileDir>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\sam3n\include\sam3n1a.h</StartupFileDir>
	<ConfigurableProperties>
        <PropertyGroups>
          <PropertyGroup>
           </PropertyGroup>
        </PropertyGroups>
    </ConfigurableProperties>
	  	<CoreFramework>
	  	<CopyJobs>
			<CopyJob>
				<SourceFolder>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\sam3n</SourceFolder>
				<FilesToCopy>-*gcc*;-*iar*;*.h;*.c</FilesToCopy>
				<TargetFolder>Utils</TargetFolder>
				<ProjectInclusionMask>*.c</ProjectInclusionMask>
				<AutoIncludeMask>*.h</AutoIncludeMask>
				<SimpleFileConditions>
				</SimpleFileConditions>
			</CopyJob>		
		</CopyJobs>	
		
		</CoreFramework>
	<Subfamilies>			
	</Subfamilies>
	     <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix</Key>
          <Value>sam3n</Value>
        </SysVarEntry>
				<SysVarEntry>
          <Key>com.sysprogs.atmel.sam32.dirutils</Key>
          <Value>sam</Value>
        </SysVarEntry>
		     <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix_sam0</Key>
          <Value>no</Value>
        </SysVarEntry>

     </AdditionalSystemVars>

</FamilyDefinition>
