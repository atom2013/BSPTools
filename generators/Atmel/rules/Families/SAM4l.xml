<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>SAM4L</Name>
	<DeviceRegex>^SAM4L.*</DeviceRegex>
	<FamilySubdirectory>SAM4L</FamilySubdirectory>
	<PrimaryHeaderDir>$$BSPGEN:INPUT_DIR$$</PrimaryHeaderDir>
	<StartupFileDir>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\sam4l\include\sam4l.h</StartupFileDir>
  	<CoreFramework>
	  	<CopyJobs>
			<CopyJob>
				<SourceFolder>$$BSPGEN:INPUT_DIR$$\sam\utils\cmsis\sam4l</SourceFolder>
				<FilesToCopy>-*gcc*;-*iar*;*.h;*.c</FilesToCopy>
				<TargetFolder>Utils</TargetFolder>
				<ProjectInclusionMask>-*\dffl.c;-*\osc.c;-*\pll.c;*.c</ProjectInclusionMask>
				<AutoIncludeMask>*.h</AutoIncludeMask>
				<SimpleFileConditions>
				</SimpleFileConditions>
			</CopyJob>		
		</CopyJobs>	
	</CoreFramework>
	<Subfamilies>			
	</Subfamilies>
     <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.atmel.sam32._header_prefix</Key>
          <Value>sam4l</Value>
        </SysVarEntry>
     </AdditionalSystemVars>
	 	
</FamilyDefinition>
