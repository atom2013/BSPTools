				<AutoIncludeMask>-*instance*;-*component*;*.h</AutoIncludeMask>
				<SimpleFileConditions>
				</SimpleFileConditions>
			</CopyJob>		
		</CopyJobs>			
		</CoreFramework>
	<Subfamilies>			
	</Subfamilies>
