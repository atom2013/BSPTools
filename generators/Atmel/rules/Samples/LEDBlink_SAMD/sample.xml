<?xml version="1.0"?>
<EmbeddedProjectSample xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>LEDBlink</Name>
  <Description>This is a very basic sample project - a classical blinking LED example.</Description>
	<ConfigurableProperties>
	<PropertyGroups>
	<PropertyGroup>
	  <Properties>
		<PropertyEntry xsi:type="Enumerated">
		  <Name>LED Port Group</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.LEDPORT</UniqueID>
		  <Description>Specifies the port group where the LED is connected</Description>
		  <SuggestionList>
			<Suggestion><UserFriendlyName>Port A</UserFriendlyName><InternalValue>A</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>Port B</UserFriendlyName><InternalValue>B</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>Port C</UserFriendlyName><InternalValue>C</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>Port D</UserFriendlyName><InternalValue>D</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>Port E</UserFriendlyName><InternalValue>E</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>Port F</UserFriendlyName><InternalValue>F</InternalValue></Suggestion>
		  </SuggestionList>
		  <DefaultEntryIndex>0</DefaultEntryIndex>
		  <AllowFreeEntry>false</AllowFreeEntry>
		</PropertyEntry>
		<PropertyEntry xsi:type="Enumerated">
		  <Name>LED Port Number</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.LEDBIT</UniqueID>
		  <Description>Specifies the port number of the LED port</Description>
		  <SuggestionList>
			<Suggestion><UserFriendlyName>2</UserFriendlyName><InternalValue>02</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>4</UserFriendlyName><InternalValue>04</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>5</UserFriendlyName><InternalValue>05</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>8</UserFriendlyName><InternalValue>08</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>9</UserFriendlyName><InternalValue>09</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>14</UserFriendlyName><InternalValue>14</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>15</UserFriendlyName><InternalValue>15</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>24</UserFriendlyName><InternalValue>24</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>25</UserFriendlyName><InternalValue>25</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>26</UserFriendlyName><InternalValue>26</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>27</UserFriendlyName><InternalValue>27</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>28</UserFriendlyName><InternalValue>28</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>30</UserFriendlyName><InternalValue>30</InternalValue></Suggestion>
			<Suggestion><UserFriendlyName>31</UserFriendlyName><InternalValue>31</InternalValue></Suggestion>
		  </SuggestionList>
		  <DefaultEntryIndex>2</DefaultEntryIndex>
		  <AllowFreeEntry>false</AllowFreeEntry>
		</PropertyEntry>
		<PropertyEntry xsi:type="Integral">
		  <Name>Delay cycles</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.DELAYCYCLES</UniqueID>
		  <Description>Specifies the delay (in empty loop cycles) between LED toggling events</Description>
		  <DefaultValue>100000</DefaultValue>
		</PropertyEntry>
	  </Properties>
	  <CollapsedByDefault>false</CollapsedByDefault>
	</PropertyGroup>
	</PropertyGroups>
	</ConfigurableProperties>
  <MCUFilterRegex>SAMD.*</MCUFilterRegex>
  <DoNotUpgradeCToCpp>false</DoNotUpgradeCToCpp>
  <RequiredFrameworks>
    <string>com.sysprogs.arm.atmel.drivers.sam0</string>
  </RequiredFrameworks>
  <DefaultConfiguration>
    <Entries>
      <KeyValue>
        <Key>com.sysprogs.arm.atmel.drivers.sam0.port</Key>
        <Value>1</Value>
      </KeyValue>
      <KeyValue>
        <Key>com.sysprogs.arm.atmel.drivers.sam0.system</Key>
        <Value>1</Value>
      </KeyValue>
    </Entries>
  </DefaultConfiguration>
</EmbeddedProjectSample>