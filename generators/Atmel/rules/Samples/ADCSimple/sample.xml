<?xml version="1.0"?>
<EmbeddedProjectSample xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>ADCSimple</Name>
	<Description>This is a very basic sample project - a classical ADC Driver on example.</Description>
	<MCUFilterRegex>SAM.*</MCUFilterRegex>
	<RequiredFrameworks>
		<string>com.sysprogs.arm.atmel.drivers.sam0</string>	
	</RequiredFrameworks>
	<ConfigurableProperties>
	<PropertyGroups>
	<PropertyGroup>
	  <Properties>
	  </Properties>
	</PropertyGroup>
	</PropertyGroups>
	</ConfigurableProperties>
	<DefaultConfiguration>
    <Entries>
      <KeyValue>
        <Key>com.sysprogs.arm.atmel.drivers.sam0.adc</Key>
        <Value>1</Value>
      </KeyValue>
      <KeyValue>
        <Key>com.sysprogs.arm.atmel.drivers.sam0.system</Key>
        <Value>1</Value>
      </KeyValue>
    </Entries>
	</DefaultConfiguration>

</EmbeddedProjectSample>